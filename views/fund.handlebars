
<!-- ////////////////////////////////////////////////////////////////////////////////// -->
<div class="container mx-auto">
    <div class="flex flex-wrap">
        <div class="w-full px-4 py-6">
            <div class="">
                <div class="fund max-w-[1280px]">
                    <div class="year">
                        <i class="fa-solid fa-angle-left"></i>
                        <div class="calendar">
                            <img
                                    width="40px"
                                    height="40px"
                                    src="/media/fund/google-calendar.png"
                                    alt="calendar img"
                                    class="calendar-img"
                            />
                            <h1 id="current-year" class="current-year">2022</h1>
                        </div>
                        <i class="fa-solid fa-angle-right"></i>
                    </div>
                    <div class="fund-details">
                        <div>Title</div>
                        <div>Date</div>
                        <div>Status</div>
                    </div>
                    {{#each fund}}
                        <div class="month">
                            <div>{{title}}</div>
                            <div>{{date}}</div>
                            <div class="comming-status">{{status}}</div>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</div>
<!-- /////////////////////////////////////////////////////////////////////////////////// -->